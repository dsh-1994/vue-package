{
  "extends": ["@vue/tsconfig/tsconfig.dom.json", "./tsconfig.base.json"],
  "include": [
    "env.d.ts",
    "./typings/**/*",
    "./types/**/*",
    "src/**/*",
    "src/**/*.vue",
    "modules/**/*",
    "packages/**/*"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo"
  }
}
